{
  "$schema": "./koryphaios-schema.json",
  "providers": {},
  "agents": {
    "manager": {
      "model": "claude-sonnet-4-20250514",
      "reasoningEffort": "high"
    },
    "coder": {
      "model": "claude-sonnet-4-20250514",
      "maxTokens": 16384
    },
    "task": {
      "model": "o4-mini",
      "maxTokens": 8192
    }
  },
  "server": {
    "port": 3000,
    "host": "localhost"
  },
  "dataDirectory": ".koryphaios",
  "contextPaths": [
    ".cursorrules",
    "CLAUDE.md",
    "AGENTS.md",
    ".opencode.json",
    "CONVENTIONS.md"
  ]
}
